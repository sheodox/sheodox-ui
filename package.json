{
  "name": "sheodox-ui",
  "version": "0.17.11",
  "description": "Common UI for my projects.",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "test": "npm run style:check && npm run lint && svelte-check --ignore package && tsc --noEmit -p .",
    "build:dev": "svelte-kit package",
    "build": "svelte-kit package",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "lint": "eslint ./src/**/*.{ts,svelte}",
    "prepare": "husky install",
    "build:release": "npm run build && npm test && cd package && npm publish"
  },
  "author": "sheodox",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "normalize.css": "^8.0.1",
    "svelte": "^3.48.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sheodox/sheodox-ui"
  },
  "homepage": "https://github.com/sheodox/sheodox-ui",
  "sideEffects": [
    "./style.scss"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.45",
    "@sveltejs/kit": "^1.0.0-next.338",
    "@tsconfig/svelte": "^2.0.1",
    "@types/sass": "^1.43.0",
    "@types/sass-loader": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^7.0.0",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.32.13",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.10",
    "typescript": "^4.4.4"
  }
}
