{
  "name": "sheodox-ui",
  "version": "0.15.3",
  "description": "Common UI for my projects.",
  "type": "module",
  "scripts": {
    "test": "npm run style:check && svelte-check --ignore package && tsc --noEmit -p .",
    "build:dev": "svelte-kit package",
    "build": "svelte-kit package",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "prepare": "husky install",
    "build:release": "npm run build && npm test && cd package && npm publish"
  },
  "author": "sheodox",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "normalize.css": "^8.0.1",
    "svelte": "^3.44.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sheodox/sheodox-ui"
  },
  "homepage": "https://github.com/sheodox/sheodox-ui",
  "sideEffects": [
    "./style.scss"
  ],
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0-next.192",
    "@tsconfig/svelte": "^2.0.1",
    "@types/sass": "^1.43.0",
    "@types/sass-loader": "^8.0.2",
    "css-loader": "^5.2.4",
    "husky": "^7.0.0",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.0",
    "style-loader": "^2.0.0",
    "svelte-check": "^2.2.7",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.9.8",
    "svelte2tsx": "^0.4.8",
    "typescript": "^4.4.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  }
}
